<Window x:Class="cube_lut_wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cube_lut_wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Name="GenerateImage" Click="GenerateImage_Click" Width="80" Height="30" Margin="20">Hald clut</Button>
            <Button Name="GenerateCube" Click="GenerateCube_Click" Width="100" Height="30"  Margin="20">Generate Cube</Button>
        </StackPanel>
        
    </Grid>
</Window>
